    <AssemblyName>System.Windows.Forms.DataVisualization.Design</AssemblyName>
    <OutputType>Library</OutputType>
    
    <CreateAsmMeta>true</CreateAsmMeta>
    <CreateMetaAssembly>true</CreateMetaAssembly>
    <RefAsmGenKeepOption>All</RefAsmGenKeepOption>

    <SignAssemblyAttribute>true</SignAssemblyAttribute>

    <AssemblyKeyFile>$(DevDivToolsPath)\35MSSharedLib1024.snk</AssemblyKeyFile>
    
    <GenerateAssemblyRefs>true</GenerateAssemblyRefs>
    <AllowPartialTrustedCalls>false</AllowPartialTrustedCalls>
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
    <DefineConstants>$(DefineConstants);WINFORMS_CONTROL</DefineConstants>
    <RootNamespace>System.Web.UI.Design.DataVisualization.Charting</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(DebugBuild)' == 'true' ">
    <DefineConstants>$(DefineConstants);CODE_ANALYSIS</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PublishPartCompiled Condition="'$(FEATURE_PAL)'==''" Include="$(NDP_TLB_PUBLISH_PUBLIC)" />
  </ItemGroup>
  <ItemGroup>

